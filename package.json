    "contributor": [
        {
            "name": "Axel ROTTHIER"
        }
    ],
    "dependencies": {
        "node-red": "2.2.2",
        "node-red-contrib-google-cloud": "^0.0.26"
    },
    "engines": {
        "node": ">=12"
    }
}